
<h1>Codeial / <%=title%></h1>

<section id="feed-posts">
    <h1>Posts</h1>
    <form action="/posts/create" id="new-post-form" method="post">
          <textarea name="content" id="" cols="30" rows="4" placeholder="type here..."></textarea>
          <input type="submit" value="Post">
    </form>
   <div id="posts-list-container">
    <ul>
        <% for(post of posts){%>
         <li>
            <%= post.content %>
         </li>
        <%}%>
    </ul>

   </div>
</section>
